<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.toshiba.happyfamily">

    <application
        android:allowBackup="true"
        android:icon="@drawable/family"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Family" />
        <activity android:name=".Sayed" />
        <activity android:name=".Parent" />
        <activity android:name=".Brother" />
        <activity android:name=".Sister" />
        <activity android:name=".History" />
        <activity android:name=".Bwife" />
        <activity android:name=".Bson"></activity>
    </application>

</manifest>